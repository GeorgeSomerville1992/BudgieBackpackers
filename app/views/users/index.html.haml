.container
  .row
    .large-6.columns
      .page-header
        %h1 All Users
      #table-responsive
        %table{class: "table table-striped table-hover"}
          %tr
            %th Name
            %th Email
            %th Friendship
            - @users.each do |user|
              - unless user == current_user
                %tr
                  %td= user.name
                  %td= user.email
                  / 

  %br/