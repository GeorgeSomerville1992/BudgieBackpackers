- content_for :templates do      
  %script#hostel-show-template{type: "text/template"}
    %dl.accordion{"data-accordion" => ""}
      %dd.accordion-navigation
        %a{href: "#panel1"} <%= indx %> <%= name %> <%= proximityDistance %> away
        #panel1.content
          %dl.tabs{"data-tab" => ""}
            %dd.active
              %a{href: "#panel2-1"} General
            %dd
              %a{href: "#panel2-2"} Available room details
            %dd
              %a{href: "#panel2-3"} Trip advisor rating
          
          .tabs-content
            #panel2-1.content.active
              %p Description: <%= shortDescription %> ...
              %p Low Rate: <%= lowRate %> <%= rateCurrencyCode %>
              %p Address: <%= address1 %>
              %p Postal Code: <%= postalCode %>
            #panel2-2.content
              %p Max room Occupancy: <%= RoomRateDetailsList.RoomRateDetails.maxRoomOccupancy %>
              %p Room Description: <%= RoomRateDetailsList.RoomRateDetails.roomDescription %>
              %p Features: 
             

            #panel2-3.content
              :plain
                <img src="<%= tripAdvisorRatingUrl %>" <br/>
              %p Number of Reviews: <%= tripAdvisorReviewCount %>
            #panel2-4.content
              %p Fourth panel content goes here...
  %script#hostel-show-cheapest-hotels{type: "text/template"}
    .large-3.columns
      .flip{style: "width: 200px; height: 350px;"}
        .flip-front
          %ul.pricing-table
            %li.title <%= name %>
            %li.price <%= lowRate %>
            %li.bullet-item Address: <%= address1 %>
            %li.bullet-item Postcode: <%= postalCode %>
            %li.bullet-item  
              :plain
                <img src="http://images.travelnow.com/<%= thumbNailUrl %>" />
              
               
            // what does plain do???
        .flip-back  
          %ul.pricing-table{"data-equalizer-watch" => ""} 
            %li.bullet-item
            %li.cta-button
              %a.button{href: "#"} Buy Now
              %p contact number unavailable 
              :plain
                <a href="https://twitter.com/share" class="twitter-share-button" data-url="http://www.expedia.co.uk/" data-text="Wow I have just found this amazing deal!!! for <%= lowRate %> per night at <%= name %> in <%= city %> #deals"  counturl= data-lang="en">Tweet</a> 

              :javascript
                !function(d,s,id){
                var js,fjs=d.getElementsByTagName(s)[0];
                if(!d.getElementById(id)){
                js=d.createElement(s);js.id=id;js.src="https://platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");

    

